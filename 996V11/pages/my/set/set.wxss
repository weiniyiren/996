/* pages/my/set/set.wxss */
@import "../../pub/header/header.wxss";
.weui-cell_switch{
  width:750rpx;
height:100rpx;
background:rgba(255,255,255,1);
box-shadow:0px 1rpx 0px 0px rgba(230,230,230,1);

}
.weui-cell__fl{
  float: left;
overflow: hidden;
}
.weui-cell__fl image{
  float: left;
  width: 30rpx;
  height: 30rpx;
  margin: 35rpx auto 35rpx 24rpx;
}
.weui-cell__fl text{
font-size:34rpx;
font-family:Adobe Heiti Std R;
font-weight:bold;
color:rgba(51,51,51,1);
line-height: 100rpx;
float: left;
margin-left: 19rpx;
}
.nav_item .weui-cell__fl text{
  font-size:30rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(51,51,51,1);
margin-left: 84rpx;
}
.weui-cell__ft{
  float: right;
  overflow: hidden;
}
.weui-cell__ft switch{
  float: right;
  margin: 35rpx 25rpx auto 21rpx;
zoom: .6;
}
.weui-cell__ft text{
 float: right;
 font-size:28rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(153,153,153,1);
line-height: 100rpx;
}
.nav {
  overflow: hidden;
}