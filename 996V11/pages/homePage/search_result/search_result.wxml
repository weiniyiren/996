<!--pages/homePage/search_result/search_result.wxml-->
 <import src="../../pub/header/header.wxml" />
<template is="header" data="{{header}}"  />
<view class='result'>
  <image src='{{userCoverLink}}'></image>
  <view class='name'>{{userName}}</view>
  <view class="position">{{userPosition}}</view>
  <view class='occupation'>
  <!-- <text wx:for="occupation">{{item}}</text> -->
  <text>{{industry}}</text>
  </view>
  <view class='btn' data-userId="{{addUserId}}" bindtap="addUser">添加好友</view>
</view>