/* pages/homePage/Editing/Editing.wxss */
@import "../../pub/header/header.wxss";
.touxian{
  width:749rpx;
height:140rpx;
background:rgba(255,255,255,1);
overflow: hidden;
margin-bottom: 40rpx;
}
page{
  background: #f2f2f2;
}
.touxian_left{
  float: left;
  /* margin: 56rpx auro auto 29rpx; */
  font-size:30rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(12,12,12,1);
line-height: 140rpx;
padding-left: 29rpx;
}
.touxian_right{
  float: right;
  width:90rpx;
height:90rpx;
background:rgba(234,234,234,1);
border-radius:50%;
margin: 25rpx 28rpx 25rpx auto;
}
.touxian_right image{
  width: 100%;
  height: 100%;
  border-radius:50%;
}
.nav{
  width:750rpx;
height:91rpx;
background:rgba(255,255,255,1);
overflow: hidden;
border-bottom: 1rpx solid #f2f2f2;

}
.nav_left{
float: left;
font-size:30rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(12,12,12,1);
line-height: 91rpx;
margin-left: 29rpx;
}
.nav_right{
float: right;
line-height: 91rpx;
font-size:30rpx;
font-family:PingFang-SC-Medium;
font-weight:500;
color:rgba(153,153,153,1);
margin-right: 28rpx;
}
.nav_right image{
  width:10rpx;
  height:19rpx; 
  float: right;
  margin: 36rpx 26rpx 36rpx auto;
}
.nav_right text{
  float: right;
  max-width: 400rpx;
  margin-right: 26rpx;
  text-align: right;
  overflow: hidden;
  
}
